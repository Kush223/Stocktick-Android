<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/stressed_about_finance"
    app:startDestination="@id/welcomeFragment">

    <fragment
        android:id="@+id/page1"
        android:name="com.example.stocktick.ui.mutual_funds.stressed_about_finance.fragments.Page1"
        android:label="Page1" >
        <action
            android:id="@+id/action_page1_to_page2"
            app:destination="@id/page2" />
    </fragment>
    <fragment
        android:id="@+id/welcomeFragment"
        android:name="com.example.stocktick.ui.mutual_funds.stressed_about_finance.fragments.WelcomeFragment"
        android:label="fragment_welcome"
        tools:layout="@layout/fragment_welcome" >
        <action
            android:id="@+id/action_welcomeFragment_to_page1"
            app:destination="@id/page1" />
    </fragment>
    <fragment
        android:id="@+id/page2"
        android:name="com.example.stocktick.ui.mutual_funds.stressed_about_finance.fragments.Page2"
        android:label="fragment_page2"
        tools:layout="@layout/fragment_page2" />
</navigation>