<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MutualFundCard">
        <attr name="image" format="reference"/>
        <attr name="let1" format="string"/>
        <attr name="let2" format="string"/>
        <attr name="buttonText" format="string"/>
    </declare-styleable>

    <declare-styleable name="AssetCard">
        <attr name="imgsrc" format="reference"/>
        <attr name="title" format="string"/>
    </declare-styleable>

    <declare-styleable name="NeumorphEdittext">
        <attr name="text" format="string"/>
        <attr name="textColor" format="color"/>
        <attr name="hint" format="string"/>
        <attr name="usePrefix" format="boolean"/>
        <attr name="prefix" format="string"/>
        <attr name="inputType" format="enum">
            <enum name="email" value="1"/>
            <enum name="number" value="2"/>
            <enum name="decimal" value="3"/>
            <enum name="string" value="4"/>
        </attr>
    </declare-styleable>
</resources>
